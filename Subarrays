#include<iostream>
#include<vector>
using namespace std;

int main(){

	vector<int> v = {1,2,3,4,5,6,7};

	for(int i = 0; i < v.size(); i ++){      // chooses the number
		for(int j = i; j < v.size(); j ++){	 // till where
			for(int k = i; k < j; k ++){     // prints
				cout<<v[k]<<" ";
			}

			cout<<"\n";
		}
	}
}

//reverse

for(int i = 0; i < v.size(); i ++){
		for(int j = v.size(); j >= 0; j --){
			for(int k = i; k < j; k ++){
				cout<<v[k]<<" ";
			}

			cout<<"\n";
		}
	}
